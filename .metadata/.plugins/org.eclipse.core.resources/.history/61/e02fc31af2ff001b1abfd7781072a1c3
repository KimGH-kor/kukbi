package ch02;

import java.util.Scanner;

public class 퀴즈3 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		StringBuffer sb = new StringBuffer();
		
		String x = sc.next();
		int x1[] = new int[x.length()];
		String y = sc.next();
		int y1[] = new int[y.length()];
	
		int arr[] = new int[(x.length()>y.length()?x.length():y.length())+1];

		//각 수를 반대로 배열에 넣는다
		for(int i = x.length()-1, j = 0; i >= 0; i--, j++) {
			x1[i] = ((int)x.charAt(j)-48);

		}
		for(int i = y.length()-1, j = 0; i >= 0; i--, j++) {
			y1[i] = y.charAt(j)-48;
		}

		//각자리 더하고 10이 넘으면 i+1값에 +1 해준다.
		if(x1.length < y1.length) {
			for(int i = 0; i < x1.length; i++) {
				if(x1[i]+y1[i] >= 10) {
					arr[i+1] += 1;
					arr[i] += (x1[i]+y1[i])%10;
				}else {
					arr[i] += x1[i]+y1[i];
				}
			}
			
			for(int j = y1.length-1, k = 0; k < y1.length - x1.length; k++,j--) {
				arr[j] += y1[j];
			}
		}else if(x1.length > y1.length){
			for(int i = 0; i < y1.length; i++) {
				if(x1[i]+y1[i] >= 10) {
					arr[i+1] += 1;
					arr[i] = (x1[i]+y1[i])%10;
				}else {
					arr[i] = x1[i]+y1[i];
				}
			}
			for(int j = x1.length-1, k = 0; k < x1.length - y1.length; k++,j--) {
				arr[j] = x1[j];
			}
		}else {
			for(int i = 0; i < y1.length; i++) {
				if(x1[i]+y1[i] >= 10) {
					arr[i+1] += 1;
					arr[i] = (x1[i]+y1[i])%10;
				}else {
					arr[i] = x1[i]+y1[i];
				}
			}
		}
		int count = 0;
		if(arr[arr.length-1] == 0) {
			count++;
		}
		if(count == 0) {
			for(int i = arr.length-1; i >= 0; i--) {
				System.out.print(arr[i]);
			}
		}else {
			for(int i = arr.length-2; i >= 0; i--) {
				System.out.print(arr[i]);
		}
	}
}
}