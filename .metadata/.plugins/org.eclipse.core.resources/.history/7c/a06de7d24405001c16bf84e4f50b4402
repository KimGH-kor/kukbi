package ch06;

class Account{
	//º”º∫
	String accno;
	String name;
	int balance;
	
	void deposit(int amount) {
		balance += amount;
	}
	void withdraw(int amount) {
		balance -= amount;
	}
}

public class acount {
	public static void main(String[] args) {
		Account acc = new Account();
		acc.accno = "1234-1234";
		acc.name = "±Ë∞Ê»Ø";
		acc.balance = 10000;
	}

}
