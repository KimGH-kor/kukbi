package ch06;

class Account{
	//속성
	String accno;
	String name;
	int balance;
	
	void deposit(int amount) {
		balance += amount;
	}
	void withdraw(int amount) {
		balance -= amount;
	}
}

public class acount {
	public static void main(String[] args) {
		//객체 생성
		Account acc1 = new Account();
		acc1.accno = "1234-1234";
		acc1.name = "김경환";
		acc1.balance = 10000;

		Account acc2 = new Account();
		acc2.accno = "4321-4321";
		acc2.name = "양지현";
		acc2.balance = 20000;
		
		acc1.deposit(20000);
		
	}

}
