package ch07;

public class LEC {
	public static void main(String[] args) {
		Point obj1 = new Point(10, 20);
		Point obj2 = obj1;
		System.out.printf("obj1 = (%d, %d) %n", obj1.a, obj1.b);
		System.out.printf("obj1 = (%d, %d) %n", obj2.a, obj2.b);
		obj2.a = 30;
		System.out.printf("obj1 = (%d, %d) %n", obj1.a, obj1.b);
		System.out.printf("obj1 = (%d, %d) %n", obj2.a, obj2.b);
	}
}
class Point{
	int a;
	int b;
	public Point(int a, int b) {
		this.a = a;
		this.b = b;
	}
}