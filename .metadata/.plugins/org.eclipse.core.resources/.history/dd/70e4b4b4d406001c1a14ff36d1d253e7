package ch07;

public class LEC {
	public static void main(String[] args) {
		Point obj1 = new Point(10, 20);
		
		System.out.printf("obj1 = (%d, %d) %n", obj1.a, obj1.b);
		rearrange(obj1);
		System.out.printf("obj1 = (%d, %d) %n", obj1.a, obj1.b);
		
	}
	static void swap(int a, int b) {
		int t = a;
		a = b;
		b = t;
	}
	
	public static void rearrange(Point point) {
		point.a = 30;
		point.b = 40;
	}
}
class Point{
	int a;
	int b;
	public Point(int a, int b) {
		this.a = a;
		this.b = b;
	}
}