package ch06;

class Account{
	//加己
	String accno;
	String name;
	int balance;
	
	void deposit(int amount) {
		balance += amount;
	}
	void withdraw(int amount) {
		balance -= amount;
	}
}

public class acount {
	public static void main(String[] args) {
		//按眉 积己
		Account acc = new Account();
		acc.accno = "1234-1234";
		acc.name = "辫版券";
		acc.balance = 10000;
		
	}

}
